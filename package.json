{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with VanillaJS & Node.js",
  "main": "init.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 1 --ignore '.scss' --ignore 'static'",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js','src/postcss.config.js'",
    "copyFrontEnd": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyFrontEnd",
    "prebuild": "rm -rf build && rm -rf src/static",
    "start": "node build/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/copyx/wetube-clone.git"
  },
  "author": "Jingi Kim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/copyx/wetube-clone/issues"
  },
  "homepage": "https://github.com/copyx/wetube-clone#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "aws-sdk": "^2.792.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.1.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@prettier/plugin-pug": "^1.9.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mini-css-extract-plugin": "^1.2.0",
    "nodemon": "^2.0.5",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  }
}
